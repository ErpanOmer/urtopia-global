var totalPrice = products["7633738727640"].variants[0].price;
setTotalPriceContent();
function setTotalPriceContent(){
  let currencyCode = Shopify.currency.active;
  let showTotalPrice = Intl.NumberFormat('de-DE', {style: 'currency', currency: currencyCode})
.format(totalPrice/100.00);
  let showComparePrice = Intl.NumberFormat('de-DE', {style: 'currency', currency: currencyCode})
.format((totalPrice + 60000)/100.00);
  $("#total-price-pc>span").html(showTotalPrice);
  $("#total-price-mb>span").html('<span class="price">'+showTotalPrice+'<span class="compare-price">'+showComparePrice+'</span></span>');
}
//choose a
var list = document.querySelectorAll(".Accessories-item")
var accessoriesIconList = document.querySelectorAll(".Accessories-item .icon")
var cartList = {"items":[]};
let accNameList=[];
var i;
	for(i = 0;i<list.length;i++){
	  (function(index){
  		list[index].addEventListener("click", function(){
		  show(index)
		})
	  })(i)
	}
let setAccessoriesDetailShow = function(arr){
  if(Array.prototype.isPrototypeOf(arr) && arr.length != 0){
	let temp = document.createElement('div')
	temp.classList="Accessories-list";
	arr.forEach((item)=>{
	  let node = document.createElement('div')
	  node.classList="option"
	  node.innerHTML=item;
	  temp.appendChild(node);
	})
	document.querySelector(".Accessories-options").replaceChild(temp,document.querySelector(".Accessories-list"))
	document.querySelector(".Accessories-options").style.display="block"
  }
  else{
	document.querySelector(".Accessories-options").style.display="none"
  }
}
let show = function(index){
  let accessoriesVariant=products[list[index].getAttribute("data-product-id")].variants[0];
  if(list[index].style.borderColor=="rgb(0, 0, 0)")
	{
	  list[index].style.borderColor = "#E0E0E0";
	  list[index].style.background = "rgba(240,240,240,0)";
	  list[index].style.borderWidth = "1px";
	  list[index].style.padding="10px 15px 10px 1px";
	  accessoriesIconList[index].style.background="#f7f7f7"
	  accessoriesIconList[index].innerHTML = '<i class="fa-solid fa-plus"></i>'
	  var rmI= 0;
	  for(rmI=0;rmI<cartList.items.length;rmI++){
		  if(cartList.items[rmI].id==accessoriesVariant.id)
			{
			  cartList.items.splice(rmI,1);
			  accNameList.splice(rmI,1);
			  break;
		  }
	  }
	  totalPrice -= accessoriesVariant.price;
	}
  else{
	list[index].style.borderColor = "#000";
	list[index].style.background = "rgba(240,240,240,1)";
	list[index].style.borderWidth = "2px";
	list[index].style.padding="9px 14px 9px 0px";
	accessoriesIconList[index].style.background="#fff"
	accessoriesIconList[index].innerHTML = '<i class="fa-solid fa-check"></i>'
	cartList.items.push({"id":accessoriesVariant.id,'quantity': 1});
	accNameList.push(accessoriesVariant.name);
	totalPrice += accessoriesVariant.price;
  }
  setAccessoriesDetailShow(accNameList);
  setTotalPriceContent();
}


